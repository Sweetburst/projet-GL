<html>
	<head>
		<script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>
	</head>
	<body>

		<style>
			.example-wrapper {
				margin: 1em auto;
				max-width: 800px;
				width: 95%;
				font: 18px / 1.5 sans-serif;
			}
			.example-wrapper code {
				background: #F5F5F5;
				padding: 2px 6px;
			}

			html,
			body {
				font-size: 62.5%;
				height: 100%;
			}

			.wrapper {
				width: 100%;
				height: 100%;
				background: #956AC4;
				display: flex;
				align-items: center;
				justify-content: center;
			}

			

				img {
					width: 60%;
					position: absolute;
					top: 50%;
					left: 50%;;
					transform: translate(-50%, -50%);
					height:50%
				}
			}

		</style>


		{% if app.user %}
			<main class="wrapper">
				<div class="loader">
					<img class="loader__animated" src="https://image.flaticon.com/icons/svg/561/561071.svg" alt="">
				</div>
			</main>

			<a id="aHidenChangePageAutoClick" {% if is_granted('ROLE_ADMIN') %} href="{{ path('back_office_test') }}" {% elseif is_granted('ROLE_USER') %} href="{{path('demoPage')}}" {% endif %} style='display:none'>
				abcderff
				{# {{ dump(app.user) }} #}
			{% endif %}

		</main>

		<script>
			window.location.href = $('#aHidenChangePageAutoClick').attr('href'); // il se click automatiquement apres le scan et transfer to page result.
		</script>
	</body>
</html>
